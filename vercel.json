{  
    "version": 2,  
    "builds": [  
        {  
            "src": "./index.js",  
            "use": "@vercel/node"  
        }  
    ],  
    "functions": {  
    "src/routes/invoice.route.js": {  
        "maxDuration": 60
    }  
    },  
    "routes": [  
    {  
        "src": "/(.*)",  
        "dest": "/"  
    }  
    ]  
}
